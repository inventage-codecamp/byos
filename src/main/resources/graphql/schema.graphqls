type Query {
    bookById(id: ID): Book
    authorById(id: ID): Author
    authors: [Author]
    books: [Book]
    test: Test
}

type Test {
    value: String
}

type Book {
    id: ID
    title: String
    publishedin: Int
    author: Author
    authors: [Author] # for now
}

type Author {
    id: ID
    firstName: String
    lastName: String
    books: [Book]
    distinguished: Boolean
}
